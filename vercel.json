{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://community-empowering-2-0.onrender.com/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/index.html"
        }
    ]
}